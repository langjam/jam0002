; Custom namespaces

namespace test {
	function testFunc print "Hello, world!"
	set testVar = 10 + 15

	testFunc
	print testVar
}

test~testFunc
print test~testVar

; You can reopen namespaces later on to edit them

namespace test {
	set testVar = -42
}

test~testFunc
print test~testVar

; Or even nest namespaces

namespace outer {
	namespace inner {
		set var = 1999
	}
}

print outer~inner~var
